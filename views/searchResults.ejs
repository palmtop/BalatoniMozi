<!DOCTYPE html>
<html>
<head>
    <title>Movie Search Results</title>
</head>
<body>
    <h1>Movie Search Results</h1>
    <% if (movies && movies.length > 0) { %>
        <ul>
            <% movies.forEach(function(movie) { %>
                        <%= movie.title %> <%= movie.genreStr%> (<%=  movie.release_date.substring(0,4) %>) <br/>
 
            <% }); %>
        </ul>
    <% } else { %>
        <p>No movies found.</p>
    <% } %>
</body>
</html>